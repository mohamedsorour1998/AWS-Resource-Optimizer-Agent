# AgentCore Runtime Environment Variables
# Copy this to .env and fill in your values

# Gateway Configuration
GATEWAY_URL=https://your-gateway-id.gateway.bedrock-agentcore.us-east-1.amazonaws.com/mcp

# Cognito OAuth Configuration
COGNITO_CLIENT_ID=your_client_id
COGNITO_CLIENT_SECRET=your_client_secret
COGNITO_TOKEN_URL=https://your-domain.auth.us-east-1.amazoncognito.com/oauth2/token

# AWS Region (optional, defaults to us-east-1)
AWS_REGION=us-east-1